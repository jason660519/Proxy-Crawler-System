# 環境變量配置示例
# 複製此文件為 .env 並填入您的 API 金鑰

# ProxyScrape API 金鑰
PROXYSCRAPE_API_KEY=your_proxyscrape_api_key_here

# GitHub Personal Access Token
GITHUB_TOKEN=your_github_token_here

# Shodan API 金鑰
SHODAN_API_KEY=your_shodan_api_key_here

# Censys API 憑證 (免費版本每月 250 次搜尋)
CENSYS_API_ID=your_censys_api_id_here
CENSYS_API_SECRET=your_censys_api_secret_here

# 加密金鑰 (用於加密存儲 API 金鑰)
PROXY_MANAGER_ENCRYPTION_KEY=your_encryption_key_here

# 數據庫配置
DATABASE_URL=postgresql://user:password@localhost:5432/proxy_manager
REDIS_URL=redis://localhost:6379/0

# 日誌配置
LOG_LEVEL=INFO
LOG_FILE=logs/proxy_manager.log

# 代理池配置
MAX_PROXY_POOL_SIZE=1000
PROXY_VALIDATION_INTERVAL=3600

# 掃描配置
SCAN_TIMEOUT=5
MAX_CONCURRENT_SCANS=100
